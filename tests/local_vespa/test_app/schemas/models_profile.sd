schema models_profile {
    document models_profile {
        # E.g. "primaries", "experimentals"
        field id type string {
            indexing: attribute | summary | index
        }

        # E.g. "primaries", "experimentals"
        field name type string {
            indexing: attribute | summary
        }

        struct concept_version {
            field concept_id type string {}
            field revision_id type string {}
            field canonical_id type string {}
        }
        
        # If a concept has no entry in the profile, that means it's not to be displayed.
        #
        # E.g. <Q990, <Q990, 1610, n2nuerdn>>
        field concepts_versions type map<string, concept_version> {
            indexing: summary

            struct-field key {
                indexing: attribute | summary
            }
            struct-field value {
                indexing: attribute | summary
            }
        }
    }
}
